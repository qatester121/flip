<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="2" name="Suite" verbose="2" parallel="tests">

    <test name="Test on Chrome">
        <parameter name="browser" value="Chrome"/>
        <parameter name="browserVersion" value="83.0"/>
        <parameter name="os" value="OS X"/>
        <parameter name="osVersion" value="Catalina"/>
        <parameter name="resolution" value="1920x1080"/>
        <classes>
            <class name="TC01Login"/>
            <class name="com.novohq.app.TC02Dashboard"/>
            <class name="com.novohq.app.TC03Activity"/>
            <class name="com.novohq.app.TC04MakeAPayment"/>
            <class name="com.novohq.app.TC05AccountInfo"/>
            <class name="com.novohq.app.TC06ChangePassword"/>
            <class name="com.novohq.app.TC07ForgotPassword"/>
        </classes>
    </test>

</suite>
